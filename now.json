{
    "version": 2,
    "name": "portafolio-engels",
    "alias": ["engels-prado-dev"],
    "builds": [{ "src": "index.html", "use": "@now/node" }],
    "routes": [
        {
          "src": "(.*).js", "dest": "/$1.js"
        },
        {
          "src": "(.*).json", "dest": "/$1.json"
        },
        {
          "src": "/.*", "dest": "/index.html"
        }
      ]
  }